{
  "name": "@reagentai/cli",
  "version": "0.2.0",
  "description": "Reagent cli",
  "type": "module",
  "bin": {
    "reagent": "dist/reagent.js"
  },
  "scripts": {
    "build-client": "vite build -c vite.client.config.js",
    "build-bin": "vite build -c vite.bin.config.js",
    "build": "pnpm run \"/^build-[client|bin]/\""
  },
  "exports": {
    "./reagent": "./dist/reagent.js",
    "./entry-client": "./dist/entry-client.js"
  },
  "publishConfig": {
    "directory": "dist",
    "bin": {
      "reagent": "reagent.js"
    },
    "exports": {
      "./reagent": "./reagent.js",
      "./entry-client": "./entry-client.js"
    },
    "linkDirectory": false
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.24.5",
    "@hono/node-server": "^1.11.2",
    "@hono/vite-dev-server": "^0.12.1",
    "@radix-ui/react-toast": "^1.1.5",
    "@reagentai/react": "workspace:^",
    "@reagentai/reagent": "workspace:^",
    "@reagentai/serve": "workspace:^",
    "@vitejs/plugin-react": "^4.3.0",
    "builtins": "^5.1.0",
    "commander": "^12.1.0",
    "dagre": "^0.8.5",
    "hono": "^4.3.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "reactflow": "^11.11.3",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.11",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/generator": "^7.24.5",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.6.8",
    "@types/dagre": "^0.7.52",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "vitest": "^1.6.0"
  }
}
